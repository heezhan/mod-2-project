<% if notice %>
    <div><%= notice %></dive>
<% end %>

<h1>Login</h1>

<%= form_tag "/login" do %>
    <%= label :username, "Username" %>
    <%= text_field_tag :username %>
    <%= label :password, "Password" %>
    <%= password_field_tag :password %>
    <%= submit_tag "Login" %>
<% end %>


